<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
          http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>InsertManager</servlet-name>
        <servlet-class>com.servlet.InsertManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertManager</servlet-name>
        <url-pattern>/InsertManager</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EditManager</servlet-name>
        <servlet-class>com.servlet.EditManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditManager</servlet-name>
        <url-pattern>/EditManager</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteManager</servlet-name>
        <servlet-class>com.servlet.DeleteManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteManager</servlet-name>
        <url-pattern>/DeleteManager</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>LoginFilter</filter-name>
        <filter-class>com.user.filter.LoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>LoginFilter</filter-name>
        <url-pattern>/Admin/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>InsertNav</servlet-name>
        <servlet-class>com.servlet.InsertNav</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertNav</servlet-name>
        <url-pattern>/InsertNav</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EditNav</servlet-name>
        <servlet-class>com.servlet.EditNav</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditNav</servlet-name>
        <url-pattern>/EditNav</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteNav</servlet-name>
        <servlet-class>com.servlet.DeleteNav</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteNav</servlet-name>
        <url-pattern>/DeleteNav</url-pattern>
    </servlet-mapping>
</web-app>